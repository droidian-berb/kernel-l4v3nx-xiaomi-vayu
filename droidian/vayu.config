# Ensure overlay gets built
CONFIG_BUILD_ARM64_DT_OVERLAY=y

# WLAN
CONFIG_QCA_CLD_WLAN=y

# OTG
CONFIG_USB_OTG=y

# CIFS
CONFIG_CIFS=y

## Virtualization
CONFIG_VIRTUALIZATION=y
CONFIG_KVM=y
## BRANCH_PREDICTOR needed to avoid KVM build crash
CONFIG_HARDEN_BRANCH_PREDICTOR=y

# dm crypt
CONFIG_DM_CRYPT=y
